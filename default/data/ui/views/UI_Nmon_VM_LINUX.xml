where (nodename = Linux) (Linux.hostname=$hostname-prefilter$) ($frameID$) ($osfilter$) ($hostname$) `$timefilter$(Linux)` groupby _time, "Linux.frameID", "Linux.hostname" prestats=true span=1m
| bucket _time span=1m
| stats dedup_splitvals=t `VM_Linux(max)` by _time, "Linux.frameID", "Linux.hostname"
| rename Linux.* AS *
| stats $statsmode$(*) AS * by frameID, hostname</query>
