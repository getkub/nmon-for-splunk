    <row>
        <panel>

            <single>
                <search id="HostsCount">
                    <query>| inputlookup nmon_inventory | search OStype="Solaris" | stats count | rangemap field=count severe=0-0 default=guarded</query>
                </search>
                <option name="linkView">search</option>
                <option name="classField">range</option>
                <option name="beforeLabel">There are</option>
                <option name="afterLabel">Solaris Host(s)</option>
                <option name="underLabel">KNOWN IN NMON INVENTORY</option>
                <option name="field">count</option>
                <option name="drilldown">all</option>
                <option name="linkFields">result</option>
                <option name="linkSearch">| inputlookup nmon_inventory | search OStype="Solaris" | fields hostname,OStype,Solaris_sunOS_version,Solaris_version,cpu_cores,Processor,Solaris_processor_clockspeed,nmon_version</option>
            </single>

        </panel>
    </row>

    <row>
        <panel>
            <html>

                <div class="imgheader">
                    <img src="../../static/app/nmon/icons/color_theme/info2.png" alt="info"/>
                    <h1>APPLICATION INFORMATION:</h1>
                </div>

                <div class="imgminiheader">
                    <h4>
                        <i>
                            Data Categories:  <a href="search?q=search%20eventtype=nmon:*" target="_blank">Explore All</a> /
                            Event Types: <a href="search?q=search%20eventtype%3Dnmon:performance" target="_blank">nmon:performance</a> (Performance data),
                            <a href="search?q=search%20eventtype%3Dnmon:config" target="_blank">nmon:config</a> (Configuration data),
                            <a href="search?q=search%20eventtype%3Dnmon:collect" target="_blank">nmon:collect</a> (Nmon raw data generation),
                            <a href="search?q=search%20eventtype%3Dnmon:processing" target="_blank">nmon:processing</a> (Nmon raw data processing)
                            &#160;-&#160;&#160;&#160;<a href="Help" target="_blank">About Nmon Performance Monitor</a>
                        </i>
                    </h4>
                </div>


            </html>
            <single>
                <search base="Global">
                    <query>fields summary</query>
                </search>
                <option name="linkView">search</option>
                <option name="underLabel">DATE OF FIRST AND LAST PERF EVENT</option>
                <option name="drilldown">none</option>
                <option name="refresh.auto.interval">60</option>
                <option name="drilldown">all</option>
                <option name="linkFields">result</option>
                <option name="linkSearch">| `indexes_datestats` | where (sourcetype == "nmon_data") | eval summary="Starts at:" + 'First Event' + " # " + "Ends at:" + 'Last Event'</option>
            </single>
            <single>
                <search ref="Hosts with data within last 7 days"></search>
                <option name="linkView">search</option>
                <option name="afterLabel">Host(s) with activity</option>
                <option name="underLabel">IN THE LAST 7 DAYS</option>
                <option name="refresh.auto.interval">60</option>
                <option name="field">count</option>
                <option name="drilldown">all</option>
                <option name="linkFields">result</option>
                <option name="linkSearch">| savedsearch "Hosts with data within last 7 days"</option>
            </single>
            <single>
                <search ref="Volume of Data indexed Today"></search>
                <option name="linkView">search</option>
                <option name="afterLabel">MB Indexed</option>
                <option name="underLabel">TODAY / EVOLUTION VS YESTERDAY</option>
                <option name="refresh.auto.interval">60</option>
                <option name="field">value</option>
                <option name="drilldown">all</option>
                <option name="linkFields">result</option>
                <option name="linkSearch">| savedsearch "Volume of Data indexed Today"</option>
            </single>
            <single>
                <search ref="Number of Errors in Data Processing or Data Collect since last 24 Hours"></search>
                <option name="linkView">search</option>
                <option name="afterLabel">Errors Reported</option>
                <option name="underLabel">PROCESSING/COLLECT LAST 24 HOURS</option>
                <option name="refresh.auto.interval">60</option>
                <option name="field">value</option>
                <option name="drilldown">all</option>
                <option name="linkFields">result</option>
                <option name="linkSearch">(eventtype=nmon:processing OR eventtype=nmon:collect error) OR (index=_internal sourcetype=splunkd ERROR ExecProcessor nmon) NOT ("There is no python in" OR "python: not found") earliest="-24h" latest="now"</option>
            </single>
        </panel>
    </row>

